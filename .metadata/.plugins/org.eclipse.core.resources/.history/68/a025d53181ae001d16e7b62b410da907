package com.techlabs.inheritnce;

public class InheritanceTest {

	public static void main(String[] args) {
		Student student1=new Student("AAA","AAA",10,80.5);
		Employee employee1=new Employee("BBB","BBB","BB05",55000);
		System.out.println("Student Information");
		System.out.println("Name:"+student1.getName());
		System.out.println("Address:"+student1.getAddress());
		System.out.println("Roll Number: "+student1.getRno());
		System.out.println("Percentage:"+student1.getPerc());
		System.out.println("Emolpoyee Information");
		System.out.println("Name:"+employee1.getName());
		System.out.println("Address:"+employee1.getAddress());
		System.out.println("Employee ID: "+employee1.getId());
		System.out.println("Salary:"+employee1.getSalary());

	}

}
