package com.techlabs.bmi;

public class PersonTest {

	public static void main(String[] args) {
			
		Person person=new Person("Swapnil",56,0.134);
		double bmi=person.getBMI();
		System.out.pintln(bmi);
		System.out.println("You BMI:"+String.format("%.2f",bmi));
	    System.out.println("Your Boy Category:"+person.getBodyCategory(bmi));
		

	}

}
